{
    "name": "techapi/mongo-laravel",
    "description": "Integração Mongo",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Mongo\\": "src/"
      }
    },
    "require": {
      "php": "^8.2",
      "illuminate/support": "^11.0 || ^12.0",
      "jenssegers/mongodb": "^5.4"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Mongo\\Providers\\MongoServiceProvider"
        ],
        "aliases": {
          "Mongo": "Mongo\\Facades\\Mongo"
        }
      }
    }
  }